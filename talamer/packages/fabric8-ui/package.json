  "description": "The CodeReady Toolchain front end.",
  "keywords": [
    "angular2",
    "typescript"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "e2e": "gulp e2e",
    "e2e:clean": "gulp e2eClean",
    "e2e:last": "gulp e2eStart",
    "e2e:reconfig": "gulp e2eReconfig",
    "analyze": "osio-scripts analyze",
    "build": "CI=false STATIC_DIR=_static osio-scripts build",
    "postinstall": "node ./scripts/postinstall.js",
    "prod": "osio-scripts prod",
    "semantic-release-origin-master": "semantic-release --branch origin/master",
    "start": "osio-scripts start",
    "start:prod": "npm run prod",
    "test": "osio-scripts test --transformIgnorePatterns='node_modules/(?!@ngrx|ngx|fabric8|patternfly-ng|ng2|@osio)' --watchAll=false",
    "test:debug": "osio-scripts test:debug --transformIgnorePatterns='node_modules/(?!@ngrx|ngx|fabric8|patternfly-ng|ng2|@osio)' --watchAll=false ",
    "test:e2e": "npm run e2e"
  },
