{
  "name": "ngx-feature-flag-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "clean": "npm cache clear --force && npm run clean:lib && npm run clean:app",
    "clean:lib": "npm run rimraf -- projects/ngx-feature-flag/node_modules",
    "clean:app": "npm run rimraf -- package-lock.json node_modules dist src/assets/README.md",
    "build": "npm run build:lib && npm run build:app",
    "build:app": "ng build",
    "prebuild:app": "cp README.md src/assets/README.md",
    "reinstall": "npm run clean && npm install",
    "rimraf": "rimraf",
    "test": "npm run test:lib",
    "test:app": "ng test --watch=false",
    "test:lib": "ng test ngx-feature-flag --watch=false --browsers ChromeHeadlessNoSandbox",
    "test:lib:dev": "ng test ngx-feature-flag",
    "lint": "ng lint",
    "lint:app:fix": "ng lint ngx-feature-flag-app --fix",
    "lint:lib:fix": "ng lint ngx-feature-flag --fix",
    "lint:fix": "npm run lint:app:fix && npm run lint:lib:fix",
    "e2e": "ng e2e",
    "install:lib": "cd projects/ngx-feature-flag && npm i",
    "prebuild:lib": "npm run install:lib && mkdir -p projects/ngx-feature-flag/node_modules/patternfly/dist/less/bootstrap/less && cp -rf projects/ngx-feature-flag/node_modules/bootstrap/less/* projects/ngx-feature-flag/node_modules/patternfly/dist/less/bootstrap/less && mkdir -p projects/ngx-feature-flag/node_modules/patternfly/dist/less/font-awesome/less && cp projects/ngx-feature-flag/node_modules/font-awesome/less/*.less projects/ngx-feature-flag/node_modules/patternfly/dist/less/font-awesome/less/",
    "build:lib": "ng build --prod ngx-feature-flag",
    "pack:lib": "cd dist/ngx-feature-flag && npm pack",
    "package": "npm run build:lib && npm run pack:lib",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabric8-ui/ngx-feature-flag.git"
  },
  "dependencies": {
    "@angular/common": "^7.0.4",
    "@angular/core": "^7.0.4",
    "@angular/compiler": "^7.0.4",
    "@angular/animations": "^7.0.4",
    "@angular/forms": "^7.0.4",
    "@angular/http": "^7.0.4",
    "@angular/platform-browser": "^7.0.4",
    "@angular/platform-browser-dynamic": "^7.0.4",
    "@angular/router": "^7.0.4",
    "core-js": "^2.5.7",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26",
    "lodash": "^4.17.11",
    "patternfly": "^3.58.0",
    "patternfly-ng": "^4.5.1"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^7.0.4",
    "@angular-devkit/build-ng-packagr": "~0.10.6",
    "@angular-devkit/build-angular": "~0.10.6",
    "ng-packagr": "^4.4.0",
    "tsickle": ">=0.33.1",
    "tslib": "^1.9.3",
    "typescript": "~3.1.6",
    "@angular/cli": "~7.0.6",
    "@angular/language-service": "^7.0.4",
    "@types/jasmine": "~2.8.11",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~10.12.8",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~2.0.0",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.1",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "ngx-base": "^4.0.0",
    "ngx-login-client": "^2.7.0",
    "markdown-it": "8.4.2",
    "highlight.js": "9.13.1",
    "rimraf": "2.6.2",
    "semantic-release": "^15.12.0"
  },
  "release": {
    "branch": "master",
    "debug": "true",
    "pkgRoot": "dist/ngx-feature-flag"
  }
}
